{
    "manifest_version": 3,
    "name": "ProxySwitcher",
    "version": "0.1",
    "description": "Turn proxy on/off",
    "icons": {
        "48": "icons/green-circle-icon.png"
    },
    "action": {
        "default_icon": "icons/green-circle-icon.png",
        "default_title": "Proxy off"
    },
    "permissions": ["proxy", "privacy"],
    "incognito": "spanning",
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}